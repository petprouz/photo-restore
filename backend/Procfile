web: cd backend && export PYTHONPATH=$PYTHONPATH:$(pwd) && export DJANGO_SETTINGS_MODULE=restore.settings && export DJANGO_DEBUG=False && poetry install --no-dev && poetry run pip install -r requirements.txt && poetry run python manage.py migrate && poetry run python manage.py collectstatic --noinput && poetry run gunicorn restore.wsgi:application --bind 0.0.0.0:$PORT --workers 4 --threads 2 --timeout 120 --access-logfile - --error-logfile - --capture-output --enable-stdio-inheritance 